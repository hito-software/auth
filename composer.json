{
    "name": "hito-software/auth",
    "description": ":package_description",
    "keywords": [],
    "homepage": "https://github.com/hito-software/auth",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "hito-software/core": "dev-master",
        "hito-software/module": "dev-master",
        "spatie/laravel-package-tools": "^1.9.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.11",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hito\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hito\\Auth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "hito-software/composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": "Hito\\Auth\\AuthServiceProvider"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
